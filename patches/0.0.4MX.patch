+            Iterator var4 = MXConfig.getConfig().getStringList("messages.rank-reseted").iterator();
+
+            while (var4.hasNext()) {
+                String rankup = (String) var4.next();
+                player.sendMessage(ChatColor.translateAlternateColorCodes('&', rankup));
+            }
+        }
+    }
+
+}
